{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "types": ["node"]
  },
  "include": ["./**/*"],
  "exclude": ["_generated"]
}
