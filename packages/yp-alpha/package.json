{
  "name": "@yp/alpha",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.mjs",
      "require": "./dist/router/index.js"
    },
    "./config": {
      "types": "./dist/config/models.d.ts",
      "import": "./dist/config/models.mjs",
      "require": "./dist/config/models.js"
    },
    "./config/wolf-realtime-prompt": {
      "types": "./dist/config/wolf-realtime-prompt.d.ts",
      "import": "./dist/config/wolf-realtime-prompt.mjs",
      "require": "./dist/config/wolf-realtime-prompt.js"
    },
    "./convex/_generated/api": {
      "types": "./convex/_generated/api.d.ts",
      "import": "./convex/_generated/api.js",
      "require": "./convex/_generated/api.js"
    },
    "./convex/_generated/dataModel": {
      "types": "./convex/_generated/dataModel.d.ts"
    },
    "./convex/_generated/server": {
      "types": "./convex/_generated/server.d.ts",
      "import": "./convex/_generated/server.js",
      "require": "./convex/_generated/server.js"
    },
    "./convex/schema": "./convex/schema.ts",
    "./convex/users": "./convex/users.ts",
    "./convex/progress": "./convex/progress.ts",
    "./convex/gamification": "./convex/gamification.ts",
    "./convex/auth.config": "./convex/auth.config.ts",
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.js"
    },
    "./auth/client": {
      "types": "./dist/auth/client.d.ts",
      "import": "./dist/auth/client.mjs",
      "require": "./dist/auth/client.js"
    },
    "./shopify": {
      "types": "./dist/shopify/index.d.ts",
      "import": "./dist/shopify/index.mjs",
      "require": "./dist/shopify/index.js"
    },
    "./voices": {
      "types": "./dist/voices/index.d.ts",
      "import": "./dist/voices/index.mjs",
      "require": "./dist/voices/index.js"
    },
    "./convex/playbook": "./convex/playbook.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "convex:dev": "npx convex dev",
    "convex:deploy": "npx convex deploy",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.0",
    "@convex-dev/better-auth": "^0.10.9",
    "better-auth": "^1.4.10",
    "better-sqlite3": "^12.5.0",
    "convex": "^1.31.2",
    "nanoid": "^5.1.6",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
